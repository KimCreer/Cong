{
  "expo": {
    "name": "MUNTINLUPA DISTRICT OFFICE",
    "slug": "cong-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "runtimeVersion": "1.0.0",
    "jsEngine": "hermes",
    
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/app-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "assetBundlePatterns": ["**/*"],
    
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/97e74b8a-1220-4109-b05b-3a226837a558"
    },
    
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow access to your gallery to choose a profile picture."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "https://maven.transistorsoft.com/content/repositories/releases/",
              "https://www.jitpack.io"
            ],
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 34,
            "buildToolsVersion": "35.0.0",
            "extraProguardRules": "-keep class expo.modules.securestore.** { *; }"
          }
        }
      ],
      ["expo-notifications"],
      [
        "react-native-background-fetch",
        {
          "dependencyVersion": "3.9.3"
        }
      ],
      [
        "expo-secure-store",
        {
          "androidBlockedEncryptionTypes": ["AES"]
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    
    "android": {
      "package": "com.allencrer.congapp",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.WAKE_LOCK",
        "android.permission.FOREGROUND_SERVICE",
        "android.permission.DOWNLOAD_WITHOUT_NOTIFICATION",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "content",
              "mimeType": "*/*"
            },
            {
              "scheme": "file",
              "mimeType": "*/*"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    
    "web": {
      "favicon": "./assets/favicon.png"
    },
    
    "extra": {
      "eas": {
        "projectId": "9f670d69-c1bd-428c-8f91-470eaa7b613b"
      }
    },
    
    "owner": "allencrer"
  }
}